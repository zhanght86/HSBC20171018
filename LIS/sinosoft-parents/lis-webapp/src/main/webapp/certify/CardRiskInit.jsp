function initCardRiskGrid()
{
  var iArray = new Array();
  try
  {
    iArray[0]=new Array();
    iArray[0][0]="序号";
    iArray[0][1]="30px";
    iArray[0][2]=10;
    iArray[0][3]=0;

    iArray[1]=new Array();
    iArray[1][0]="单证编码";
    iArray[1][1]="100px";
    iArray[1][2]=100;
    iArray[1][3]=2;
    iArray[1][4]="CertifyCode";
    iArray[1][9]="险种信息:单证编码|NOTNULL&INT"; 		//检验格式

    iArray[2]=new Array();
    iArray[2][0]="险种编码";
    iArray[2][1]="100px";
    iArray[2][2]=100;
    iArray[2][3]=2;
    iArray[2][4]="RiskCode";
    //iArray[2][9]="险种信息:险种代码|NOTNULL&INT"; 		//检验格式
    
    iArray[3]=new Array();
    iArray[3][0]="产品代码";
    iArray[3][1]="100px";
    iArray[3][2]=100;
    iArray[3][3]=2;
    iArray[3][4]="portfolio";


    iArray[4]=new Array();
    iArray[4][0]="保费";         			//列名
    iArray[4][1]="100px";            		//列宽
    iArray[4][2]=200;            			//列最大值
    iArray[4][3]=1;              			//是否允许输入,1表示允许，0表示不允许
	iArray[4][9]="险种信息:保费|NOTNULL&NUM";   		//检验格式

    iArray[5]=new Array();
    iArray[5][0]="保费特性";         	//列名
    iArray[5][1]="100px";            	//列宽
    iArray[5][2]=60;            			//列最大值
    iArray[5][3]=2;              			//2表示是代码选择
    iArray[5][10]="PremProp";					//保费特性
    iArray[5][11]="0|^1|固定保费|^2|保费倍数|^3|任意保费";

    iArray[6]=new Array();
    iArray[6][0]="保费份额";         			//列名
    iArray[6][1]="100px";            		//列宽
    iArray[6][2]=60;            			//列最大值
    iArray[6][3]=1;              			//是否允许输入,1表示允许，0表示不允许

    CardRiskGrid = new MulLineEnter( "document" , "CardRiskGrid" );
    CardRiskGrid.mulLineCount = 5;
    CardRiskGrid.displayTitle = 1;
    CardRiskGrid.loadMulLine(iArray);
  }
  catch(ex)
  {
    alert("CardRiskInit.jsp-->初始化定额单险种信息时出错"+ex);
  }
