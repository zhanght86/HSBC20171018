					<Data type="单证页码" index="3" newdocflag="" />
					<Data type="单证版本号" index="4" newdocflag="" />
				</BarCode>
			</setting>
			<!-- 一个条码，根据字符串划分，第一部分是单证类型，第二部分是单证号码 -->
			<setting name="民生投保单设置_业务类型+版本+单证细类" barcodecount="2">
				<BarCode index="1" delitype="LENGTH" delimiter="">
					<Data type="单证细类" index="1" newdocflag="0" start="1"
						length="4" />
					<Data type="单证版本号" index="2" newdocflag="" start="5"
						length="2" />
					<Data type="单证号码" index="3" newdocflag="0" start="1"
						length="14" />
				</BarCode>
				<BarCode index="2" delitype="" delimiter="">
					<Data type="单证页码" index="1" newdocflag="" />
				</BarCode>
			</setting>
			<!-- 一个条码，独立条码，第一个是单证类型 -->
			<setting name="民生投保单设置_一个独立条码_单证类型" barcodecount="1">
				<BarCode index="1" delitype="" delimiter="">
					<Data type="单证细类" index="1" newdocflag="0" />
				</BarCode>
			</setting>
			<!-- 一个条码，独立条码，第一个是单证号码 -->
			<setting name="民生投保单设置_一个独立条码_单证号码" barcodecount="1">
				<BarCode index="1" delitype="" delimiter="">
					<Data type="单证号码" index="1" newdocflag="0" />
				</BarCode>
			</setting>
			<!-- 批量扫描分隔页 -->
				<setting name="民生投保单独立设置" barcodecount="1">
					<BarCode index="1" delitype="" delimiter="">
