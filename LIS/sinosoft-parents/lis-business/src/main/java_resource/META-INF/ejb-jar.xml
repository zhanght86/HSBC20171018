<?xml version="1.0"?>

<!DOCTYPE ejb-jar PUBLIC 
'-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN' 
'http://java.sun.com/dtd/ejb-jar_2_0.dtd'>

<ejb-jar>
	<enterprise-beans>
	   <session>
	      <display-name>BusinessFacade</display-name>
	      <ejb-name>BusinessFacade</ejb-name>
	      <home>com.sinosoft.service.BusinessHome</home>
	      <remote>com.sinosoft.service.Business</remote>
	      <ejb-class>com.sinosoft.service.BusinessFacade</ejb-class>
	      <session-type>Stateless</session-type>
	      <transaction-type>Container</transaction-type>
	   </session>
	</enterprise-beans>
</ejb-jar>
